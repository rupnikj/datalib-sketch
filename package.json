{
  "name": "datalib-sketch",
  "version": "0.0.6",
  "description": "Probabilistic data structures for large or streaming data sets.",
  "keywords": [
    "data",
    "sketch",
    "approximate",
    "streaming",
    "count min",
    "count mean min",
    "bloom filter",
    "ngram"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/uwdata/datalib-sketch.git"
  },
  "author": {
    "name": "Jeffrey Heer",
    "url": "http://idl.cs.washington.edu"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "browserify": "latest",
    "browserify-shim": "latest",
    "chai": "latest",
    "istanbul": "latest",
    "jshint": "latest",
    "mocha": "latest",
    "uglify-js": "latest"
  },
  "main": "src/index.js",
  "scripts": {
    "deploy": "npm run lint && npm run test && scripts/deploy.sh",
    "lint": "jshint src/",
    "test": "mocha --recursive test/",
    "cover": "istanbul cover _mocha -- --recursive test/",
    "build": "browserify src/index.js --debug --standalone dl.sketch -o datalib-sketch.js",
    "postbuild": "uglifyjs datalib-sketch.js -c -m -o datalib-sketch.min.js"
  }
}
